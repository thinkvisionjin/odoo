<odoo>
    <data> 
           <record id="PACKAGE_STATUS_STARTUP" model="xmgl.packagestatus"> 
             <field name="name">包件建档</field> 
             <field name="code">STARTUP</field> 
           </record>
           <record id="PACKAGE_STATUS_PUBLIC" model="xmgl.packagestatus"> 
             <field name="name">包件公告</field> 
             <field name="code">PUBLIC</field> 
           </record>
           <record id="PACKAGE_STATUS_OPEN" model="xmgl.packagestatus"> 
             <field name="name">包件开标</field> 
             <field name="code">OPEN</field> 
           </record>
           <record id="PACKAGE_STATUS_REVIEW" model="xmgl.packagestatus"> 
             <field name="name">包件评标</field> 
             <field name="code">REVIEW</field> 
           </record>
           <record id="PACKAGE_STATUS_WINPUBLIC" model="xmgl.packagestatus"> 
             <field name="name">中标公示</field> 
             <field name="code">WINPUBLIC</field> 
           </record>
           <record id="PACKAGE_STATUS_RETURNASS" model="xmgl.packagestatus"> 
             <field name="name">退保证金</field> 
             <field name="code">RETURNASS</field> 
           </record>
           <record id="PACKAGE_STATUS_ABORT" model="xmgl.packagestatus"> 
             <field name="name">包件流标</field> 
             <field name="code">ABORT</field> 
           </record>
           <record id="PACKAGE_STATUS_ARCHIVE" model="xmgl.packagestatus"> 
             <field name="name">包件归档</field> 
             <field name="code">ARCHIVE</field> 
           </record>
           <record id="PACKAGE_STATUS_FINISH" model="xmgl.packagestatus"> 
             <field name="name">包件完成</field> 
             <field name="code">FINISH</field> 
           </record>
